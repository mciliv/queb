{
  "name": "mol",
  "version": "1.0.0",
  "description": "Molecular analysis app",
  "main": "backend/api/server.js",
  "scripts": {
    "start": "node backend/api/server.js",
    "dev": "NODE_ENV=development node backend/api/server.js",
    "dev:watch": "nodemon backend/api/server.js",
    "dev:simple": "NODE_ENV=development node backend/api/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch:unit": "jest --watch --selectProjects unit-frontend unit-backend",
    "test:watch:integration": "jest --watch --selectProjects integration",
    "test:watch:all": "jest --watchAll",
    "test:auto": "nodemon --watch backend/ --watch frontend/ --watch test/ --ext js,html,css --exec 'npm test'",
    "test:auto:unit": "nodemon --watch backend/ --watch frontend/ --ext js --exec 'npm run test:unit'",
    "test:auto:quick": "nodemon --watch backend/ --watch frontend/ --ext js --ignore test/ --exec 'npm run test:unit'",
    "clean": "pkill -f 'node.*server.js' && echo 'Cleaned up'",
    "procedure:test-livereload": "./scripts/test-livereload",
    "procedure:quick-dev": "./scripts/quick-dev",
    "procedure:debug": "./scripts/debug-pipeline",
    "test:quick": "./scripts/test-quick",
    "test:with-server": "./scripts/test-with-server",
    "test:unit": "jest --selectProjects unit-frontend unit-backend",
    "test:integration": "jest --selectProjects integration",
    "test:smoke": "jest --selectProjects smoke"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.0.0",
    "multer": "^2.0.0",
    "openai": "^4.0.0",
    "pg": "^8.11.3",
    "sharp": "^0.32.0",
    "stripe": "^14.7.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "babel-jest": "^29.7.0",
    "connect-livereload": "^0.6.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^26.1.0",
    "livereload": "^0.9.3",
    "nodemon": "^3.0.2",
    "puppeteer": "^24.15.0",
    "supertest": "^7.1.4"
  },
  "keywords": [
    "molecular",
    "analysis",
    "chemistry",
    "3d"
  ],
  "author": "",
  "license": "MIT"
}
