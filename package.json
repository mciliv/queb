{
  "name": "queb",
  "version": "1.0.0",
  "description": "Chemical contents prediction",
  "main": "src/server/api/server.js",
  "dependencies": {
    "@google-cloud/secret-manager": "^6.1.0",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "esbuild": "^0.25.8",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "openai": "^4.100.0",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "toml": "^3.0.0",
    "ws": "^8.18.0",
    "zod": "^3.25.67"
  },
  "optionalDependencies": {
    "pg": "8.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^27.0.0",
    "puppeteer": "^24.16.0",
    "supertest": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mciliv/queb.git"
  },
  "license": "MIT",
  "scripts": {
    "prepare": "git config core.hooksPath hooks",
    "start": "node src/server/api/server.js",
    "debug": "node --inspect src/server/api/server.js",
    "debug-brk": "node --inspect-brk src/server/api/server.js",
    "build": "node src/client/build-frontend.js",
    "build:watch": "node src/client/build-frontend.js --watch",
    "gcp-build": "node src/client/build-frontend.js",
    "vercel-build": "node src/client/build-frontend.js",
    "test": "jest --config tests/jest.unit.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "jest --config tests/jest.unit.js",
    "test:integration": "jest --config tests/jest.integration.js",
    "test:e2e": "jest --config tests/jest.e2e.js",
    "test:visual": "jest --config tests/jest.visual.js",
    "test:visual-mock": "MOCK_MODE=true jest --config tests/jest.visual.js",
    "test:watch": "jest --config tests/jest.unit.js --watch",
    "test:coverage": "jest --config tests/jest.unit.js --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:ci": "npm run test:unit -- --ci --reporters=default --reporters=jest-junit",
    "setup:db": "node database/setup/setup-database.js",
    "cleanup:logs": "node scripts/cleanup-logs.js",
    "check:env": "node scripts/check-env.js",
    "validate:env": "node scripts/validate-env.js",
    "deploy": "node scripts/deploy.js",
    "deploy:dev": "bash deploy/deploy.sh dev",
    "deploy:prod": "bash deploy/deploy.sh prod"
  }
}
