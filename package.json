{
  "name": "molecular-space-analyzer",
  "version": "1.0.0",
  "description": "Molecular analysis app - maximally simplified",
  "main": "functions.js",
  "scripts": {
    "start": "node backend/api/server.js",
    "dev": "scripts/dev",
    "dev:commit": "./dev-commit",
    "dev:simple": "NODE_ENV=development nodemon --watch backend --ext js,json --ignore frontend --exec \"node --inspect=9229\" backend/api/server.js",
    "dev:brk": "NODE_ENV=development nodemon --watch backend --ext js,json --ignore frontend --exec \"node --inspect-brk=9229\" backend/api/server.js",
    "build:frontend": "node scripts/build-frontend.js",
    "test": "node test/scripts/unified-test-runner.js",
    "test:quick": "node test/scripts/unified-test-runner.js --quick",
    "test:parallel": "node test/scripts/parallel-version-test.js",
    "test:versions": "node test/scripts/parallel-version-test.js main previous",
    "add:version": "./add-version",
    "auto:version": "./auto-add-version",
    "sync:versions": "scripts/sync-versions",
    "compare:versions": "./compare-versions",
    "list:versions": "scripts/list-versions",
    "clean": "scripts/clean",
    "format": "prettier --write ."
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^3.25.67"
  },
  "optionalDependencies": {
    "openai": "^4.100.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.25.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.10",
    "puppeteer": "^24.16.0",
    "supertest": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mciliv/mol.git"
  },
  "license": "MIT"
}
