{
  "name": "chemical-contents-predictor",
  "version": "1.0.0",
  "description": "Chemical contents prediction",
  "main": "src/server/api/server.js",
  "dependencies": {
    "@ai-sdk/openai": "^3.0.7",
    "@ai-sdk/xai": "^3.0.10",
    "ai": "^6.0.21",
    "cors": "^2.8.5",
    "esbuild": "^0.25.8",
    "express": "^4.21.2",
    "html2canvas": "^1.4.1",
    "http-errors": "^2.0.1",
    "lodash": "^4.17.23",
    "node-fetch": "^3.3.2",
    "openai": "^6.15.0",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "toml": "^3.0.0",
    "ws": "^8.18.0",
    "zod": "^3.25.67"
  },
  "optionalDependencies": {
    "pg": "8.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "autoprefixer": "^10.4.23",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "puppeteer": "^23.0.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^4.1.18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mciliv/queb.git"
  },
  "license": "MIT",
  "scripts": {
    "prepare": "git config core.hooksPath hooks",
    "start": "node src/server/api/server.js",
    "debug": "node --inspect src/server/api/server.js",
    "debug-brk": "node --inspect-brk src/server/api/server.js",
    "build": "node src/client/build-frontend.js",
    "build:watch": "node src/client/build-frontend.js --watch",
    "test": "npx jest --config tests/jest.unit.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:llm": "npm run test:llm-mocked && npm run test:smoke",
    "test:llm-full": "npm run test:llm-mocked && npm run test:smoke && npm run test:llm-integration",
    "test:unit": "npx jest --config tests/jest.unit.js",
    "test:unit:cacao-regression": "npx jest --config tests/jest.unit.js --runInBand tests/unit/prompt-engine.test.js tests/unit/structuralizer.test.js",
    "test:integration": "npx jest --config tests/jest.integration.js",
    "test:e2e": "npx jest --config tests/jest.e2e.js",
    "test:smoke": "npx jest --config tests/jest.unit.js --testPathPattern=smoke --testTimeout=60000",
    "test:llm-mocked": "npx jest --config tests/jest.unit.js --testPathPattern=openai-service",
    "test:llm-integration": "npx jest --config tests/jest.integration.js --testPathPattern=end-to-end-llm",
    "test:visual": "npx jest --config tests/jest.visual.js",
    "test:visual-mock": "MOCK_MODE=true npx jest --config tests/jest.visual.js",
    "test:watch": "npx jest --config tests/jest.unit.js --watch",
    "test:coverage": "npx jest --config tests/jest.unit.js --coverage",
    "test:debug": "NODE_OPTIONS='--inspect-brk' npx jest --runInBand",
    "test:ci": "npm run test:unit -- --ci --reporters=default --reporters=jest-junit",
    "setup:db": "node database/setup/setup-database.js",
    "cleanup:logs": "node scripts/cleanup-logs.js",
    "check:env": "node scripts/check-env.js",
    "validate:env": "node scripts/validate-env.js",
    "validate:ai": "node scripts/validate-ai-setup.js",
    "test:db-recommender": "node tests/test-database-recommender.js",
    "dev": "concurrently \"npm run debug\" \"npm run build:watch\" --names \"server,build\" --prefix name",
    "dev:full": "concurrently \"npm run debug\" \"npm run build:watch\" \"npm run test:watch\" --names \"server,build,tests\" --prefix name",
    "debug:test": "NODE_OPTIONS='--inspect-brk' npx jest --runInBand --testPathPattern=${npm_config_test:-\"\"}",
    "debug:current": "NODE_OPTIONS='--inspect-brk' npx jest --runInBand --testPathPattern=\"${file:-\"\"}\""
  }
}
