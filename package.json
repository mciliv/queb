{
  "name": "queb",
  "version": "1.0.0",
  "description": "Molecular analysis app - queb",
  "main": "src/server/api/server.js",
  "dependencies": {
    "@google-cloud/secret-manager": "^6.1.0",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "esbuild": "^0.25.8",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.100.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "toml": "^3.0.0",
    "ws": "^8.18.0",
    "zod": "^3.25.67"
  },
  "optionalDependencies": {
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "babel-jest": "^29.7.0",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^27.0.0",
    "puppeteer": "^24.16.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mciliv/queb.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest --config tests/jest.config.js",
    "test:quick": "jest --config tests/jest.config.js tests/suites/unit/keyboard-shortcuts.test.js",
    "test:integration": "jest --config tests/jest.config.js --testMatch='**/suites/integration/*.test.js'",
    "dev": "node scripts/dev-chrome.js",
    "dev:server": "node --max-old-space-size=1024 --expose-gc src/server/api/server.js",
    "dev:chrome": "node scripts/dev-chrome.js",
    "dev:full": "npx concurrently \"npm run dev:server\" \"npm run build:watch\"",
    "start": "node src/server/api/server.js",
    "build": "node src/client/build-frontend.js",
    "build:watch": "node src/client/build-frontend.js --watch",
    "clean": "node scripts/cleanup-processes.js",
    "status": "node scripts/check-status.js", 
    "deploy": "node scripts/deploy.js",
    "setup-aliases": "node scripts/setup-aliases.js",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  }
}
